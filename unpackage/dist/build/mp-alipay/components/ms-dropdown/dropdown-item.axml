<view class="dropdown-item _div"><view data-event-opts="{{[['tap',[['changePopup',['$event']]]]]}}" class="dropdown-item__selected" onTap="__e"><block a:if="{{$slots.title}}"><slot name="title"></slot></block><block a:else><block><view class="selected__name">{{title?title:selectItem.text}}</view><view class="{{(('selected__icon')+' '+(showClass==='show'?'up':'down'))}}"><label class="iconfont _span">î¡‘</label></view></block></block></view><block a:if="{{showList}}"><view class="dropdown-item__content" style="{{'top:'+(contentTop+'px')+';'}}"><view class="{{(('list')+' '+showClass)}}"><block a:if="{{$slots.default}}"><slot></slot></block><block a:else><block><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['choose',['$0'],[[['list','',index]]]]]]]}}" class="list__option" onTap="__e"><view>{{item.text}}</view><block a:if="{{item.value===value}}"><icon type="success_no_circle" size="15"></icon></block></view></block></block></block></view><block a:if="{{showList}}"><view data-event-opts="{{[['tap',[['closePopup',['$event']]]]]}}" class="{{(('dropdown-mask')+' '+showClass)}}" onTap="__e"></view></block></view></block></view>