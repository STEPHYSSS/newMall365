<view class="container"><view style="height:55px;" class="_div"><uni-search-bar vue-id="d9d538ec-1" radius="{{100}}" value="{{searchBar}}" data-event-opts="{{[['^input',[['__set_model',['','searchBar','$event',[]]],['addressSearch']]],['^cancel',[['canceladdressSearch']]]]}}" onInput="__e" onCancel="__e" onVueInit="__l"></uni-search-bar></view><block a:if="{{!showSearch}}"><view class="_div"><map id="map" scale="{{map.scale}}" show-location="{{map.showLocation}}" longitude="{{map.longitude}}" latitude="{{map.latitude}}" width="{{map.width}}" height="{{map.height}}" markers="{{map.markers}}" onRegionChange="mapChange"><cover-image class="icon-img" src="/static/icon_position.png"></cover-image></map><view class="footer bg-ff font-26"><scroll-view class="scroll" scroll-y="{{true}}" scroll-top="{{scrollTop}}"><view><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['bindAddress',[index]]]]]}}" class="padding border-b position-r" onTap="__e"><view class="row">{{item.title}}</view><view class="row color-99">{{item.address}}</view><block a:if="{{checked===index}}"><icon class="icon_circle" type="success" color="#E74246" size="22"></icon></block></view></block></view></scroll-view></view></view></block><block a:if="{{showSearch}}"><view class="_div"><view class="bg-ff"><view class="padding color-99 font-26">从下面列表中选择</view><block a:for="{{listArea}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['addressFun',['$0'],[[['listArea','',index]]]]]]]}}" class="padding border-t list position-r" onTap="__e"><view class="padding-b">{{item.title}}</view><view class="color-99">{{item.address}}</view></view></block><block a:if="{{pageStatus&&list.length==0}}"><view class="icon_img_tip"><image src="/icon/icon_tip.png"></image><view>快去添加地址吧~~</view></view></block></view></view></block></view>