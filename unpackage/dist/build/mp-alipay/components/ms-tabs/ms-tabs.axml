<block a:if="{{list.length>0}}"><view class="tabBlock"><scroll-view scroll-x="true" scroll-with-animation="{{true}}" scroll-left="{{tabsScrollLeft}}" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" onScroll="__e"><view class="tab" id="tab_list"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('tab__item')+' '+[(currentIndex===index)?'tab__item--active':''])}}" style="{{'color:'+(currentIndex===index?''+itemColor:'')+';'}}" id="tab_item" data-event-opts="{{[['tap',[['select',['$0',index],[[['list','',index]]]]]]]}}" onTap="__e"><view class="tab__item-title"><slot name="title" title="{{item.title}}"></slot></view><block a:if="{{!showTitleSlot}}"><view class="tab__item-title">{{''+item.title+''}}</view></block></view></block></view><view class="tab__line" style="{{'background:'+(lineColor)+';'+('width:'+(lineStyle.width)+';')+('transform:'+(lineStyle.transform)+';')+('transition-duration:'+(lineStyle.transitionDuration)+';')}}"></view></scroll-view></view></block>